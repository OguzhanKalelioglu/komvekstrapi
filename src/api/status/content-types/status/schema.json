{
  "kind": "collectionType",
  "collectionName": "statuses",
  "info": {
    "singularName": "status",
    "pluralName": "statuses",
    "displayName": "Truck Status",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "serialnumber": {
      "type": "string",
      "required": true
    },
    "production_start_date": {
      "type": "date"
    },
    "production_finish_date": {
      "type": "date"
    },
    "ship_reservation": {
      "type": "date"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "demontaj",
        "kaynak",
        "polyester",
        "boya",
        "elektrik",
        "doseme",
        "kontrol"
      ],
      "required": true
    },
    "customers": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::customer.customer",
      "inversedBy": "truck_statuses"
    },
    "model_no": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "serialnumber"
    }
  }
}
